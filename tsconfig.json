{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": [
      "es6",
      "es2015",
      "dom"
    ],
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
