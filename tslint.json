{
  "extends": "tslint-microsoft-contrib",
  "rules": {
    "no-bitwise": false,
    "no-relative-imports": false,
    "no-suspicious-comment": false,
    "no-console": false,
    "import-name": true,
    "variable-name": [true, "allow-leading-underscore"],
    "no-implicit-dependencies": [true, "dev"],
    "no-submodule-imports": [true, "csv-parse/lib/sync", "gt-js-common/lib/coders/HexCoder", "gt-js-common/lib/hash/DataHash"]
  }
}
